   ],
   "source": [
    "a = 0\n",
    "b = math.pi/2\n",
    "n = int(input(\"Enter no. of Trapezoids : \"))\n",
    "h = (b-a)/n\n",
    "sum_ = equation(a)\n",
    "a += h\n",
    "while (a < b):\n",
    "    sum_ += 2*equation(a)\n",
    "    a += h\n",
    "sum_ += equation(b)\n",
    "ans = sum_ * (h/2)\n",
    "print(ans)"
   ]
