   "source": [
    "A = [4, 1, 44, 5, -1, 6, 222, 91]\n",
    "x = int(input(\"Enter value to be search : \"))\n",
    "n = len(A)\n",
    "i = 0\n",
    "while (i < n):\n",
    "    if A[i] == x:\n",
    "        print(f\"Found at index {i}\")\n",
    "        break\n",
    "    i = i + 1\n",
    "if i == n:\n",
    "    print(\"Not Found\")"
   ]
